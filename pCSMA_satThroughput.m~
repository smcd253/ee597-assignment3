% constants
d = 50 * 10^-6;
Ts = 9000 * 10^-6;
Tc = 8750 * 10^-6;
num_users = 100
% note: p is same for all users (i.e. p = 1/n)

% hold on
for n = 1 : num_users
%     fprintf("st = %f\n", saturation_throughput (n, 1/n, d, Ts, Tc));

end
% hold off

% pCSMA saturation throughput function definition
function st = saturation_throughput(n, p, d, Ts, Tc)
    exp_success = Ts*n*p*(1-p)^(n-1);
    
st = n + p + d + Ts + Tc;
end